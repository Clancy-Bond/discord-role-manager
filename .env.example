# Your Discord bot token (from Developer Portal > Bot > Token)
DISCORD_TOKEN=your-bot-token-here

# Your bot's application/client ID (from Developer Portal > General Information)
CLIENT_ID=your-client-id-here

# Your bot's client secret (from Developer Portal > OAuth2 > Client Secret)
CLIENT_SECRET=your-client-secret-here

# Web dashboard port (default: 3001)
PORT=3001

# Session secret (any random string)
SESSION_SECRET=change-this-to-a-random-string
